CREATE TABLE BOOKING_INFO
  ( CUSTOMER_ID INT AUTO_INCREMENT PRIMARY KEY, 
    FNAME VARCHAR(50), 
    LNAME VARCHAR(50), 
    BDATE DATE, 
    CONTACT_NUM BIGINT, 
    EMAIL VARCHAR(255), 
    ADDRESS VARCHAR(255),  
    CITY VARCHAR(50), 
    REGION VARCHAR(50), 
    POSTAL VARCHAR(50), 
    COUNTRY VARCHAR(50), 
    DEPARTURE_DATE DATE, 
    DEPARTURE_TIME TIME,
    DEPARTURE_RDATE DATE, 
    DEPARTURE_RTIME TIME, 
    DEPARTURE_COUNTRY VARCHAR(50), 
    DESTINATION_COUNTRY VARCHAR(50), 
    FARE VARCHAR(50) );
